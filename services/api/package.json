{
  "name": "@game-guild/api",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": false,
  "license": "UNLICENSED",
  "scripts": {
    "install:deps:ubuntu": "apt update ; apt install -y build-essential cmake git",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:nodemon": "nodemon src/main.ts",
    "start:tsnode": "ts-node src/main.ts",
    "start:dev": "nest start --watch",
    "prestart:dev": "npm run syncDtos",
    "syncDtos": "npm run --prefix=../../ syncApi",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs -d ormconfig.ts",
    "migration:generate": "cd migrations && npx typeorm-ts-node-commonjs migration:generate -d ormconfig.ts",
    "migration:revert": "npm run typeorm -- migration:revert",
    "schema:drop": "npm run typeorm -- schema:drop",
    "db:diagram": "ts-node dbml.ts",
    "openapigenerator": "openapi-generator-cli generate -g typescript-axios -i swagger-spec.json -o ../web/src/api"
  },
  "dependencies": {
    "@dataui/crud": "5.2.4",
    "@dataui/crud-request": "5.2.4",
    "@dataui/crud-typeorm": "5.2.4",
    "@dataui/crud-util": "5.2.4",
    "@nestjs/common": "9.4.3",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "9.4.3",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "9.4.3",
    "@nestjs/swagger": "7.0.12",
    "@nestjs/typeorm": "9.0.1",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@sendgrid/client": "^7.7.0",
    "@sendgrid/helpers": "^7.7.0",
    "@sendgrid/mail": "^7.7.0",
    "@types/fs-extra": "^11.0.2",
    "chess.js": "^1.0.0-beta.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto-js": "^4.1.1",
    "csurf": "^1.11.0",
    "decompress": "^4.2.1",
    "dotenv": "^16.4.4",
    "ethers": "^6.4.1",
    "exec-sh": "^0.4.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^11.1.1",
    "google-auth-library": "^9.11.0",
    "helmet": "^7.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "multer": "1.4.5-lts.1",
    "nestjs-cls": "^3.5.1",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "simple-git": "^3.22.0",
    "typeorm": "^0.3.16",
    "typeorm-linq-repository": "^2.0.2",
    "typeorm-transactional": "0.4.1"
  },
  "devDependencies": {
    "@game-guild/eslint-config": "*",
    "@game-guild/jest-config": "*",
    "@game-guild/prettier-config": "*",
    "@inscreen/typeorm-uml": "^1.6.5-is1",
    "@nestjs/cli": "9.4.2",
    "@nestjs/schematics": "9.2.0",
    "@nestjs/testing": "9.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.194",
    "@types/multer": "^1.4.8",
    "@types/node": "^20.11.19",
    "@types/passport-jwt": "^3.0.10",
    "@types/passport-local": "^1.0.36",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.0.3",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
