{
  "name": "@game-guild/api",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": false,
  "licenses": [
    {
      "type": "AGPL-3.0",
      "url": "https://www.gnu.org/licenses/agpl-3.0.html"
    },
    {
